{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Secret": "secret",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Intigriti.Bus.Infrastructure.Audit": "Information",
        "Microsoft": "Information",
        "Microsoft.AspNetCore.Mvc.Internal": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "[{Timestamp:HH:mm:ss} {SourceContext}] [{Level}] {Message}{NewLine}{Exception}"
                //"formatter": "Intigriti.Serilog.JsonFormatter.IntigritiJsonFormatter, Intigriti.Serilog.JsonFormatter"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Destructure": []
  }
}
